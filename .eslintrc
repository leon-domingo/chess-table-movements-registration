{
  "globals": {
      "zzz": true
  },
  "extends": [
      "/home/leon/.eslintrc",
      "react-app",
      "react-app/jest"
  ],
  "plugins": [
      "import"
  ],
  "rules": {
      "import/no-unresolved": 0,
      "import/named": 2,
      "import/namespace": 2,
      "import/default": 2,
      "import/export": 2,
      "object-curly-spacing": [1, "always"]
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
  }
}
